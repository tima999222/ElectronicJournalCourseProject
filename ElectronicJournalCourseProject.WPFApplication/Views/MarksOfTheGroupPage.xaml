<Page x:Class="ElectronicJournalCourseProject.WPFApplication.Views.MarksOfTheGroupPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ElectronicJournalCourseProject.WPFApplication.Views"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="MarksOfTheGroupPage"
      Background="White">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"></RowDefinition>
            <RowDefinition Height="50"></RowDefinition>
        </Grid.RowDefinitions>

        <DataGrid x:Name="dataGrid" Grid.Row="0" IsReadOnly="True" AutoGenerateColumns="False">
            
        </DataGrid>

        <Button x:Name="AddButton" Grid.Row="1" Width="200" Click="AddButton_Click">Добавить оценку</Button>

    </Grid>

    
</Page>
